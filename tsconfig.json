{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@assets/icons/*": ["src/assets/icons/*"],
      "@assets/images/*": ["src/assets/images/*"],
      "@components/atoms/*": ["src/components/atoms/*"],
      "@components/molecules/*": ["src/components/molecules/*"],
      "@components/organisms/*": ["src/components/organisms/*"],
      "@components/layouts/*": ["src/components/layouts/*"],
      "@components/templates/*": ["src/components/templates/*"],
      "@huddle/molecules/*": ["src/components/molecules/huddle/*"],
      "@huddle/organisms/*": ["src/components/organisms/huddle/*"],
      "@huddle/templates/*": ["src/components/templates/huddle/*"],
      "@pages/*": ["src/pages/*"],
      "@types": ["src/types/index.ts"],
    },
  }
}
